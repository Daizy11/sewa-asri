{
  "openapi": "3.0.0",
  "servers": [
    {
      "description": "Sewa Asri Villa API",
      "url": "asritech.netlify.app"
    }
  ],
  "info": {
    "description": "This API was provided to Sewa Asri and Sewa Asri Tenant Apps",
    "version": "1.0.0",
    "title": "Sewa Asri API",
    "contact": {
      "email": "asriitech@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "tags": [
    {
      "name": "manager",
      "description": "Secured Admin-only calls"
    },
    {
      "name": "user",
      "description": "Operations available to regular developers"
    }
  ],
  "paths": {
    "/villa": {
      "get": {
        "tags": [
          "manager",
          "user"
        ],
        "summary": "Mendapatkan banyak Villa",
        "description": "path ini digunakan untuk mendapatkan semua villa",
        "responses": {
          "200": {
            "description": "Villas found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/VillaFound"
                  }
                }
              }
            }
          },
          "400": {
            "description": "bad input parameter"
          }
        }
      }
    },
    "/villa/id": {
      "get": {
        "tags": [
          "manager",
          "user"
        ],
        "summary": "Mendapatkan detail villa",
        "description": "By passing in the appropriate options, you can search for\navailable inventory in the system\n",
        "responses": {
          "200": {
            "description": "Villa found",
            "headers": {
              "Cookie": {
                "description": "Cookie yang digunakan sebagai autentikasi",
                "schema": {
                  "type": "string",
                  "example": "Bearer eyJhbGciOiJIUzI"
                }
                
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VillaFound"
                }
              }
            }
          },
          "403": {
            "description": "You do not have permission to perform this action",
            "headers": {
              "Cookie": {
                "description": "Cookie yang digunakan sebagai autentikasi",
                "schema": {
                  "type": "string",
                  "example": "undefined"
                }
                
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "status": "fail",
                    "message": "You're not logged in"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Nothing found with that data",
            "headers": {
              "Cookie": {
                "description": "Cookie yang digunakan sebagai autentikasi",
                "schema": {
                  "type": "string",
                  "example": "Bearer eyJhbGciOiJIUzI"
                }
                
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VillaNotFound"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "manager"
        ],
        "summary": "adds an inventory item",
        "operationId": "addInventory",
        "description": "Adds an item to the system",
        "responses": {
          "201": {
            "description": "item created"
          },
          "400": {
            "description": "invalid input, object invalid"
          },
          "409": {
            "description": "an existing item already exists"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/VillaCreated"
              }
            }
          },
          "description": "Inventory item to add"
        }
      },
      "patch": {
        "tags": [
          "manager"
        ],
        "summary": "adds an inventory item",
        "operationId": "addInventory",
        "description": "Adds an item to the system",
        "responses": {
          "201": {
            "description": "item created"
          },
          "400": {
            "description": "invalid input, object invalid"
          },
          "409": {
            "description": "an existing item already exists"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Villa"
              }
            }
          },
          "description": "Inventory item to add"
        }
      },
      "delete": {
        "tags": [
          "manager"
        ],
        "summary": "adds an inventory item",
        "operationId": "addInventory",
        "description": "Adds an item to the system",
        "responses": {
          "201": {
            "description": "item created"
          },
          "400": {
            "description": "invalid input, object invalid"
          },
          "409": {
            "description": "an existing item already exists"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/VillaDeleted"
              }
            }
          },
          "description": "Inventory item to add"
        }
      }
    }
  },
  "components": {
    "schemas": {
      "VillaFound": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 200
          },
          "message": {
            "type": "string",
            "example": "Villa found"
          },
          "data": {
            "type": "object",
            "example": {
              "schema": {
                "$ref": "#/components/schemas/Villa"
              }
            }
          }
        },
        "type": "object"
      },
      "VillaNotFound": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 404
          },
          "message": {
            "type": "string",
            "example": "Villa not found"
          },
          "data": {
            "type": "string",
            "example": "undefined"
          }
        },
        "type": "object"
      },
      "VillaDeleted": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 204
          },
          "message": {
            "type": "string",
            "example": "Villa not found"
          }
        },
        "type": "object"
      },
      "VillaCreated": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 204
          },
          "message": {
            "type": "string",
            "example": "Villa not found"
          }
        },
        "type": "object"
      },
      "Villa": {
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "example": "Villa Flamboyan",
            "minLength": 10,
            "maxLength": 40
          },
          "price": {
            "type": "number",
            "example": 1000000
          },
          "image": {
            "type": "array",
            "example": []
          },
          "bedroom": {
            "$ref": "#/components/schemas/Bedroom"
          },
          "createdAt": {
            "type": "string",
            "example": "24 June 2018"
          },
          "isAvailable": {
            "type": "boolean",
            "default": true
          }
        }
      },
      "VillaDetail": {
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "example": "Villa Flamboyan",
            "minLength": 10,
            "maxLength": 40
          },
          "price": {
            "type": "number",
            "example": 1000000
          },
          "image": {
            "type": "array",
            "example": []
          },
          "bedroom": {
            "$ref": "#/components/schemas/Bedroom",
            "othersFacility": {
              "type": "object",
              "$ref": "#/components/schemas/BedroomFacility"
            }
          },
          "createdAt": {
            "type": "string",
            "example": "24 June 2018"
          },
          "isAvailable": {
            "type": "boolean",
            "default": true
          }
        }
      },
      "Bedroom": {
        "required": [
          "quantity"
        ],
        "properties": {
          "quantity": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "bedSize": {
            "type": "string",
            "default": 1,
            "minLength": 1,
            "example": ["Single bed", "Double bed"]
          }
        },
        "type": "object"
      },
      "BedroomDetail": {
        "required": [
          "quantity"
        ],
        "properties": {
          "quantity": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "width": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "length": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "bedSize": {
            "type": "string",
            "default": 1,
            "minLength": 1,
            "example": ["Single bed", "Double bed"]
          }
        },
        "type": "object"
      },
      "BedroomFacility": {
        "type": "object",
        "properties": {
          "window": {
            "type": "object",
            "example": {
              "name": "Jendela",
              "status": "available"
            }
          },
          "socket": {
            "type": "object",
            "example": {
              "name": "Stop kontak",
              "status": "not available"
            },
            "default": {
              "name": "Stop kontak",
              "status": "not available"
            }
          },
          "wardrobe": {
            "type": "object",
            "example": {
              "name": "Lemari",
              "status": "not available"
            },
            "default": {
              "name": "Jendela",
              "status": "not available"
            }
          }
        }
      },
      "BathroomFacility": {
        "type": "object",
        "properties": {
          "waterHeater": {
            "type": "object",
            "example": {
              "name": "Penghangat air",
              "status": "available"
            }
          },
          "shower": {
            "type": "object",
            "example": {
              "name": "Pancuran air",
              "status": "not available"
            }
          },
          "bathtube": {
            "type": "object",
            "example": {
              "name": "Bak mandi",
              "status": "available"
            }
          },
          "squatToilet": {
            "type": "object",
            "example": {
              "name": "Toilet jongkok",
              "status": "available"
            }
          },
          "sittingToilet": {
            "type": "object",
            "example": {
              "name": "Toilet duduk",
              "status": "not available"
            }
          }
        }
      }
    }
  }
}