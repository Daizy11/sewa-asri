              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "status": 200,
                    "message": "You're not logged in"
                  }
                }
              }
            }
          },
          "500": {
            "description": "There is an error on the server side",
            "headers": {
              "Cookie": {
                "description": "Cookie yang digunakan sebagai autentikasi",
                "schema": {
