  },
  "components": {
    "schemas": {
      "VillaFound": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 200
          },
          "message": {
            "type": "string",
            "example": "Villa found"
          },
          "data": {
            "type": "object",
            "example": {
              "$ref": "#/components/schemas/Villa"
            }
          },
          "villaAvailability": {
            "type": "boolean", 
            "example": {
              "$ref": "#/components/schemas/VillaAvailability"
            }
          },
          "filter": {
            "type": "object",
            "example": {
              "$ref": "#/components/schemas/Filter"
            }
          }
        },
        "type": "object"
      },
      "VillaNotFound": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 404
          },
          "message": {
            "type": "string",
            "example": "Villa not found"
          },
          "data": {
            "type": "string",
            "example": "undefined"
          }
        },
        "type": "object"
      },
      "VillaDetailFound": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 200
          },
          "message": {
            "type": "string",
            "example": "Villa found"
          },
          "data": {
            "type": "object",
            "example": {
              "$ref": "#/components/schemas/VillaDetail"
            }
          }
        },
        "type": "object"
      },
      "VillaDeleted": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 204
          },
          "message": {
            "type": "string",
            "example": "Villa was successfully deleted"
          }
        },
        "type": "object"
      },
      "VillaCreated": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 204
          },
          "message": {
            "type": "string",
            "example": "Villa was successfully created"
          }
        },
        "type": "object"
      },
      "VillaUpdated": {
        "required": [
          "name"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 204
          },
          "message": {
            "type": "string",
            "example": "Villa was successfully updated"
          }
        },
        "type": "object"
      },
      "Villa": {
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "example": "Villa Flamboyan",
            "minLength": 10,
            "maxLength": 40
          },
          "price": {
            "type": "number",
            "example": 1000000
          },
          "image": {
            "type": "array",
            "example": []
          },
          "bedroom": {
            "$ref": "#/components/schemas/Bedroom"
          },
          "createdAt": {
            "type": "string",
            "example": "24 June 2018"
          },
          "isAvailable": {
            "type": "boolean",
            "default": true
          }
        }
      },
      "VillaDetail": {
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "example": "Villa Flamboyan",
            "minLength": 10,
            "maxLength": 40
          },
          "price": {
            "type": "number",
            "example": 1000000
          },
          "image": {
            "type": "array",
            "example": []
          },
          "bedroom": {
            "$ref": "#/components/schemas/Bedroom",
            "othersFacility": {
              "$ref": "#/components/schemas/BedroomFacility"
            }
          },
          "createdAt": {
            "type": "string",
            "example": "24 June 2018"
          },
          "attraction": {
            "$ref": "#/components/schemas/Attraction"
          },
          "isAvailable": {
            "type": "boolean",
            "default": true
          }
        }
      },
      "Bedroom": {
        "required": [
          "quantity"
        ],
        "properties": {
          "quantity": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "bedSize": {
            "type": "string",
            "default": 1,
            "minLength": 1,
            "example": ["Single bed", "Double bed"]
          }
        },
        "type": "object"
      },
      "BedroomDetail": {
        "required": [
          "quantity"
        ],
        "properties": {
          "quantity": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "width": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "length": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "bedSize": {
            "type": "string",
            "default": 1,
            "minLength": 1,
            "example": ["Single bed", "Double bed"]
          }
        },
        "type": "object"
      },
      "BedroomFacility": {
        "type": "object",
        "properties": {
          "window": {
            "type": "object",
            "example": {
              "name": "Jendela",
              "status": "available"
            }
          },
          "socket": {
            "type": "object",
            "example": {
              "name": "Stop kontak",
              "status": "not available"
            },
            "default": {
              "name": "Stop kontak",
              "status": "not available"
            }
          },
          "wardrobe": {
            "type": "object",
            "example": {
              "name": "Lemari",
              "status": "not available"
            },
            "default": {
              "name": "Jendela",
              "status": "not available"
            }
          }
        }
      },
      "BathroomFacility": {
        "type": "object",
        "properties": {
          "waterHeater": {
            "type": "object",
            "example": {
              "name": "Penghangat air",
              "status": "available"
            }
          },
          "shower": {
            "type": "object",
            "example": {
              "name": "Pancuran air",
              "status": "not available"
            }
          },
          "bathtube": {
            "type": "object",
            "example": {
              "name": "Bak mandi",
              "status": "available"
            }
          },
          "squatToilet": {
            "type": "object",
            "example": {
              "name": "Toilet jongkok",
              "status": "available"
            }
          },
          "sittingToilet": {
            "type": "object",
            "example": {
              "name": "Toilet duduk",
              "status": "not available"
            }
          }
        }
      },
      "Location": {
        "type": "object",
        "properties": {
          "city": {
            "type": "string",
            "example": "Serang Banten"
          },
          "address": {
            "type": "string",
            "example": "Jl. Raya Serang Banten No 40"
          }
        }
      },
      "Attraction": {
        "type": "object",
        "properties": {
          "photo": {
            "type": "string",
            "example": "image file"
          },
          "name": {
            "type": "string",
            "example": "Air Terjun Kawah"
          },
          "open": {
            "type": "string",
            "example": "08.00"
          },
          "close": {
            "type": "string",
            "example": "15.00"
          },
          "distance": {
            "type": "number",
            "example": 100
          }
        }
      },
      "VillaAvailability": {
        "type": "boolean",
        "example": true,
        "default": true
      },
      "Filter": {
        "type": "object",
        "properties": {
          "sortByPrice": {
            "type": "string",
            "enum": ["termurah", "termahal"],
            "example": "termurah"
          },
          "sortByLocation": {
            "type": "string",
            "enum": ["terdekat", "terjauh", "terpopuler"],
            "example": "terdekat"
          },
          "sortByRooms": {
            "type": "number",
            "example": 1
          }
        }
      }
    },
    "securitySchemes": {
      "villa_auth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "https://villabooking3.swagger.io/oauth/authorize",
            "scopes": {
              "write:villas": "modify villas in your account",
              "read:villas": "read your villas"
            }
          }
        }
      },
      "api_key": {
        "type": "apiKey",
        "name": "api_key",
        "in": "header"
      }
    }
  }
}