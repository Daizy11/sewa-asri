            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "status": 200,
                    "message": "You're not logged in"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Nothing found with that data",
            "headers": {
              "Cookie": {
                "description": "Cookie yang digunakan sebagai autentikasi",
                "schema": {
                  "type": "string",
                  "example": "Bearer eyJhbGciOiJIUzI"
                }
                
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "status": 404,
                    "message": "Villa not found",
                    "data": null
                  }
                }
              }
            }
          },
          "500": {
            "description": "There is an error on the server side",
            "headers": {
              "Cookie": {
                "description": "Cookie yang digunakan sebagai autentikasi",
                "schema": {
                  "type": "string",
                  "example": "Bearer eyJhbGciOiJIUzI"
                }
                
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "status": 500,
                    "message": "There is an error on the server side",
                    "data": null
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "villa_auth": [
              "write: villa",
              "read: villa"
            ]
          }
        ]
      },
      "post": {
        "tags": [
          "manager"
        ],
        "summary": "Membuat villa baru",
        "operationId": "addInventory",
        "description": "Adds an item to the system",
        "responses": {
          "201": {
            "description": "Villa created",
            "headers": {
              "Cookie": {
                "description": "Cookie yang digunakan sebagai autentikasi",
