  "components": {
    "schemas": {
      "BedroomFound": {
        "required": [
          "quantity"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 200
          },
          "message": {
            "type": "string",
            "example": "Bedroom detail found"
          },
          "schema": {
            "$ref": "#/components/schemas/Bedroom"
          }
        },
        "type": "object"
      },
      "BedroomCreated": {
        "required": [
          "quantity"
        ],
        "properties": {
          "status": {
            "type": "number",
            "example": 201
          },
          "message": {
            "type": "string",
            "example": "Bedroom created"
          },
          "$ref": "#/components/schemas/Bedroom"
        },
        "type": "object"
      },
      "Bedroom": {
        "required": [
          "quantity"
        ],
        "properties": {
          "quantity": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "width": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "length": {
            "type": "number",
            "default": 1,
            "minLength": 1,
            "example": 1
          },
          "bedSize": {
            "type": "string",
            "default": 1,
            "minLength": 1,
            "enum": ["Single bed", "Double bed"],
            "example": "Single bed"
          }
        },
        "type": "object"
      },
      "BedroomFacility": {
        "type": "object",
        "properties": {
          "window": {
            "type": "object",
            "example": {
              "name": "Jendela",
              "status": "available"
            }
          },
          "socket": {
            "type": "object",
            "example": {
              "name": "Stop kontak",
              "status": "not available"
            },
            "default": {
              "name": "Stop kontak",
              "status": "not available"
            }
          },
          "wardrobe": {
            "type": "object",
            "example": {
              "name": "Lemari",
              "status": "not available"
            },
            "default": {
              "name": "Jendela",
              "status": "not available"
            }
          }
        }
      },
      "BathroomFacility": {
        "type": "object",
        "properties": {
          "waterHeater": {
            "type": "object",
            "example": {
              "name": "Penghangat air",
              "status": "available"
            }
          },
          "shower": {
            "type": "object",
            "example": {
              "name": "Pancuran air",
              "status": "not available"
            }
          },
          "bathtube": {
            "type": "object",
            "example": {
              "name": "Bak mandi",
              "status": "available"
            }
          },
          "squatToilet": {
            "type": "object",
            "example": {
              "name": "Toilet jongkok",
              "status": "available"
            }
          },
          "sittingToilet": {
            "type": "object",
            "example": {
              "name": "Toilet duduk",
              "status": "not available"
            }
          }
        }
      }
    },
    "securitySchemes": {
      "bedroom_auth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "https://villabooking3.swagger.io/oauth/authorize",
            "scopes": {
              "write:bedroom": "modify bedroom in your account",
              "read:bedroom": "read your bedroom"
            }
          }
        }
      },
      "api_key": {
        "type": "apiKey",
        "name": "api_key",
        "in": "header"
      }
    }
  }
}